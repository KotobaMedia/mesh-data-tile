{
  "name": "mesh-data-tile",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./browser": {
      "types": "./dist/src/browser.d.ts",
      "import": "./dist/src/browser.js"
    }
  },
  "bin": {
    "mesh-tile": "dist/src/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "pnpm run build && node --test dist/test/*.test.js",
    "cli": "tsx src/cli.ts",
    "example:maplibre:tiles": "pnpm run build && node dist/examples/maplibre-jismesh/generate-tiles.js",
    "example:maplibre": "pnpm run example:maplibre:tiles && vite --config examples/maplibre-jismesh/vite.config.ts",
    "example:maplibre:build": "pnpm run example:maplibre:tiles && vite build --config examples/maplibre-jismesh/vite.config.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@maplibre/geojson-vt": "^5.0.4",
    "@maplibre/vt-pbf": "^4.2.1",
    "commander": "^14.0.3",
    "japanmesh": "^1.3.5"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.0.0",
    "maplibre-gl": "^5.18.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "vite": "^7.3.1"
  },
  "packageManager": "pnpm@10.29.3"
}
